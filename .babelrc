{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "Android >= 4.4",
          "iOS >= 9",
          "last 1 Explorer versions",
          "last 2 Edge versions",
          "last 2 Safari versions",
          "last 2 Chrome versions",
          "last 2 Firefox versions"
        ]
      },
      "debug": true,
      "modules": false
    }]
  ],
  "plugins": ["transform-runtime"]
}
